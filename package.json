    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^11.11.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "openai": "^4.20.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
